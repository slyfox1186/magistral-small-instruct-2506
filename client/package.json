    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "eslint-plugin-unicorn": "^59.0.1",
    "prettier": "^3.6.2",
    "terser": "^5.39.0",
    "typescript": "^5.4.5",
    "vite": "^5.4.17"
